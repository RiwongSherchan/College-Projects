<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="590.0" prefWidth="724.0" stylesheets="@../resources/styles/candidateDashboard.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.CandidateDashboardController">
   <children>
   
      	<Label layoutX="182.0" layoutY="33.0" prefHeight="111.0" prefWidth="388.0" text="            Candidate Dashboard" textAlignment="CENTER" textFill="#3e1a1a">
   		<font>
   			<javafx.scene.text.Font name="Berlin Sans FB Demi Bold" size="29.0" />
   		</font>
   	</Label>
   
      	<!-- Display Candidate Details -->
   	<Label fx:id="firstNameLabel" layoutX="459.0" layoutY="189.0" prefHeight="27.0" prefWidth="222.0" text="s" />
   	<Label fx:id="lastNameLabel" layoutX="459.0" layoutY="239.0" prefHeight="27.0" prefWidth="221.0" text="s" />
   	<Label fx:id="genderLabel" layoutX="459.0" layoutY="281.0" prefHeight="27.0" prefWidth="222.0" text="s" />
   
      	<!-- Buttons for Examination, Profile Settings, and Test Results -->
   	<Button fx:id="openExamination" layoutX="70.0" layoutY="163.0" onAction="#openExamination" prefHeight="79.0" prefWidth="210.0" text="Examination" />
   	<Button fx:id="LogoutButton" layoutX="70.0" layoutY="274.0" onAction="#logout" prefHeight="42.0" prefWidth="210.0" text="Logout" />
   </children>
</AnchorPane>
